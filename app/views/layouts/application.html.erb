  <style>
    .text-danger{
      color: #e8464e !important;  
      /*배경에 있는 붉은색*/
    }
    
    .text-white{
      color:white !important;
    }
    
    .bg-danger{
       background-color: #e8464e !important;  
    }
    .text-info{
      color: #8acbc1 !important; 
      /*모니터 속 민트색*/
    }
   .bg-info{
       background-color: #8acbc1 !important;  
    }
    .text-muted{
       color: #ABABAB !important; 
    }
    .bg-muted{
       background-color: #ABABAB !important;  
    }
    .text-muted:hover{
      color: #ABABAB !important;
      text-decoration: none !important;
    }
    a.text-muted:hover{
      color: #ABABAB !important;
      background: transparent;
      /*회색 hover 금지*/
    }
    a.text-info:hover{
      color: #8acbc1 !important;
      background: transparent;
      /*민트색 hover 금지*/
    }
    a.text-danger:hover{
      color: #e8464e !important;
      background: transparent;
      /*빨간색 hover 금지*/
    }
    
    
